{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "include": [
    "Scripts/src/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
